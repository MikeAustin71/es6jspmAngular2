{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "diagnostics": true,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "version": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/angular2/*.d.ts",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/jasmine/*.d.ts",
        ".././typings/**",
        "!.././node_modules/**"
    ],
    "files": [
        "main.ts",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/angular2/angular2.d.ts",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/angular2/http.d.ts",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/angular2/router.d.ts",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/angular2/testing.d.ts",
        ".././lib/npm/angular2@2.0.0-alpha.45/bundles/typings/jasmine/jasmine.d.ts",
        ".././typings/tsd.d.ts"
    ]
}
